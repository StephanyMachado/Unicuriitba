        return Produto;
    }

    public void setNome(String produto) {
        this.produto = produtos;
    }

    public String getAutor() {
        return Categoria;
    }

    public void setAutor(String Categooria) {
        this.autor = categoria;
    }

    @Override
    public String toString() {
        return "Produto [id=" + id + ", produto=" + produto + ", categoria=" + categoria + "]";
    }
}
